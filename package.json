{
  "name": "@omar/office",
  "private": true,
  "scripts": {
    "build": "slidev build",
    "rename": "echo 'OMARILLO' && sed -i s=/assets/=/office/assets/=g dist/*.html && mv dist/office/images dist",
    "dev": "slidev --open",
    "export": "slidev export",
    "deploy": "sed -i s=https://cdn.jsdelivr.net/gh/slidevjs/slidev/assets/favicon.png=/rete/images/solerosso.svg=g dist/*.html && gh-pages -d dist"
  },
  "dependencies": {
    "@slidev/cli": "^0.50.0",
    "@slidev/theme-default": "latest",
    "@slidev/theme-seriph": "latest",
    "gh-pages": "^6.2.0"
  },
  "devDependencies": {
    "playwright-chromium": "^1.49.1"
  }
}
